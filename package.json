{
  "name": "shallowequal",
  "version": "1.1.0",
  "description": "Like lodash isEqualWith but for shallow equal.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js test",
    "test": "jest .",
    "prepare": "npm run build && npm run pretty && npm run flow:check && npm run lint && npm run test",
    "travis": "npm run prepare",
    "pretty": "prettier --write --tab-width 2 'test/**/*.js' '*.{js,json,js.flow,md}'",
    "precommit": "lint-staged",
    "flow:check": "flow check",
    "build": "babel index.original.js > index.js"
  },
  "lint-staged": {
    "*.{js,json,js.flow,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": {
    "name": "Alberto Leal",
    "email": "mailforalberto@gmail.com",
    "url": "github.com/dashed"
  },
  "repository": "dashed/shallowequal",
  "license": "MIT",
  "files": [
    "index.js",
    "index.js.flow",
    "index.original.js"
  ],
  "keywords": [
    "shallowequal",
    "shallow",
    "equal",
    "isequal",
    "compare",
    "isequalwith"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "jest"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.12.0",
    "eslint-plugin-jest": "^22.1.3",
    "flow-bin": "^0.92.1",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  }
}
